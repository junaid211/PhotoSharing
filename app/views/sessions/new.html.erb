 <%= form_for :users, :url => login_attempt_sessions_path  do |f| %>
 <p>
      <%= f.label :email %><br>
      <%=f.text_field :email %>
    </p>
    <p>
      <%=f.label :password %> <br>
      <%= f.password_field :password %>
    </p>
    <p>

      <%= f.submit value="Sign in" %>
    </p>
<% end %>
<p><%= link_to 'home page', root_path %></p>